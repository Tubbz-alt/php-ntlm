{
    "name": "robinpowered/php-ntlm",
    "description": "Message encoder/decoder and password hasher for the NTLM authentication protocol",
    "type": "library",
    "keywords": ["ntlm", "nt", "lm", "hash", "authentication"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Trevor Suarez",
            "email": "trevor@robinpowered.com",
            "role": "Original creator"
        }
    ],
    "support": {
        "issues": "https://github.com/robinpowered/php-ntlm/issues",
        "source": "https://github.com/robinpowered/php-ntlm"
    },
    "require": {
        "php-64bit": ">=7.1.0"
    },
    "require-dev": {
        "ext-mcrypt": "*",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-spl": "*",
        "ext-openssl": "*",
        "phpunit/phpunit": "^4.7",
        "phpunit/php-code-coverage": "^2.2",
        "squizlabs/php_codesniffer": "^2.3",
        "paragonie/random_compat": "^2.0"
    },
    "suggest": {
        "paragonie/random_compat": "Allows for more cryptographically secure random data generation during the NTLM hashing process"
    },
    "autoload": {
        "psr-4": {"Robin\\Ntlm\\": "src/Robin/Ntlm/"}
    }
}
